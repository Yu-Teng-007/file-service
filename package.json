{
  "name": "file-service",
  "version": "1.0.0",
  "description": "用于静态资源存储和管理的独立文件服务",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:test": "cross-env NODE_ENV=test nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "config:validate": "node -e \"require('./dist/config/configuration').validateConfig()\"",
    "config:show": "node -e \"console.log(JSON.stringify(require('./dist/config/configuration').default(), null, 2))\"",
    "env:dev": "copy .env.development .env",
    "env:prod": "copy .env.production .env",
    "env:test": "copy .env.test .env",
    "keys:generate": "node scripts/generate-api-keys.js",
    "keys:generate:dev": "node scripts/generate-api-keys.js dev",
    "keys:generate:prod": "node scripts/generate-api-keys.js prod",
    "keys:generate:test": "node scripts/generate-api-keys.js test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.839.0",
    "@aws-sdk/s3-request-presigner": "^3.839.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "ali-oss": "^6.23.0",
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^7.0.1",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "cos-nodejs-sdk-v5": "^2.16.0-beta.3",
    "express-rate-limit": "^6.8.0",
    "file-type": "^18.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "mime-types": "^2.1.35",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^5.5.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.2",
    "tar-fs": "^3.1.0",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.5.0",
    "prettier": "^3.4.2",
    "sass": "^1.89.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.e2e-spec.ts",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/*.d.ts",
      "!**/main.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ],
    "silent": false,
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(file-type|strtok3|peek-readable|token-types)/)"
    ],
    "testTimeout": 30000
  },
  "keywords": [
    "file-service",
    "static-resources",
    "file-upload",
    "file-management",
    "nestjs"
  ],
  "author": "Your Name",
  "license": "MIT"
}
